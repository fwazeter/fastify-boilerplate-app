    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "fluent-json-schema": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "rimraf": "^5.0.5",
